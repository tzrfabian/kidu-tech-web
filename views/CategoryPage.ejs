<%- include('partials/Header') %>

<div class="container">
    <h1>Category List</h1>
    <hr>
    <a href="/course" class="btn btn-primary mb-3">All Courses</a>
    <table class="table table table-success table-striped-columns">
        <thead>
          <tr>
            <th scope="col">No.</th>
            <th scope="col">Category Name</th>
            <th scope="col">Action</th>
          </tr>
        </thead>
        <tbody>
            <% data.forEach((el, i) => { %>
                <tr>
                    <th scope="row"><%= i + 1 %></th>
                    <td><%= el.name %></td>
                    <td>
                        <a href="" class="btn btn-success">See More</a>
                    </td>
                </tr>
            <% }) %>
        </tbody>
      </table>
</div>

<%- include('partials/Footer') %>